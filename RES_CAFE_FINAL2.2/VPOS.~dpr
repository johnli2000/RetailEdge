{$R *.res}

begin
 Application.Initialize;
 Application.Title := 'V POS Software';
 if CheckScreenResolution then
    begin
     Application.CreateForm(TSecurityForm, SecurityForm);

  if SecurityForm.TestSoftDog or SecurityForm.GetNetWorkAdapters then
        begin
         GetDir(0, StartDir);
         if StartDir[Length(StartDir)] <> '\' then StartDir := StartDir + '\';
         Application.CreateForm(TDataForm, DataForm);
         Application.CreateForm(TMainForm, MainForm);
         ChDir('C:\');
         Application.Run;
        end
       else
        begin
         SecurityForm.Free;
         MessageDlg('Please call techinical support',
           mtError, [mbOk], 0);
        end;   
    end;
end.
